model Robot {
    # Stoerungen
    errors {
        persistent Errorgraph
    }

    # Gefahren
    hazards {
        "num ist zu gross" {
            num = 5
        }
    }

    # Plausibilitaetschecks
    specify {
        "Einmal inactive, immer inactive" {
            G nofaults
            => G (Robot = Inactive => (G Robot = Inactive))
        }
    }
}